models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable', admin: boolean }
    Asociacion: { nombre: string, descripcion: string, contacto: string, email: string, imagen: 'string nullable', relationships: { belongsTo: 'User:gestor', belongsToMany: 'User, Evento', morphMany: Comentario } }
    Evento: { nombre: string, descripcion: string, lugar: string, tipo: 'enum:evento,actividad', fecha_inicio: datetime, fecha_fin: datetime, accesibilidad: 'enum:socios,publico,privado,mixto', estado: 'enum:abierto,cerrado', aforo: 'integer nullable', contador_aforo: 'integer nullable', aforo_socios: 'integer nullable', contador_aforo_socios: 'integer nullable', aforo_no_socios: 'integer nullable', contador_aforo_no_socios: 'integer nullable', afotor_voluntarios: 'integer nullable', contador_aforo_voluntarios: 'integer nullable', imagen: 'string nullable', relationships: { belongsToMany: 'User, Asociacion', morphMany: Comentario } }
    Comentario: { user_id: 'foreign:User', comentario: string, fecha: datetime, valoracion: 'integer nullable', relationships: { belongsTo: User, morphTo: comentarioable } }
